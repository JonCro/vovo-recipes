    "gatsby-source-contentful": "^2.1.60",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-remark": "^2.6.34",
    "node-sass": "^7.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
